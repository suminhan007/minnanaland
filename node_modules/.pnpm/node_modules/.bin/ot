#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/minna's kits/minnanaland/node_modules/.pnpm/opentype.js@1.3.4/node_modules/opentype.js/bin/node_modules:/mnt/e/minna's kits/minnanaland/node_modules/.pnpm/opentype.js@1.3.4/node_modules/opentype.js/node_modules:/mnt/e/minna's kits/minnanaland/node_modules/.pnpm/opentype.js@1.3.4/node_modules:/mnt/e/minna's kits/minnanaland/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/minna's kits/minnanaland/node_modules/.pnpm/opentype.js@1.3.4/node_modules/opentype.js/bin/node_modules:/mnt/e/minna's kits/minnanaland/node_modules/.pnpm/opentype.js@1.3.4/node_modules/opentype.js/node_modules:/mnt/e/minna's kits/minnanaland/node_modules/.pnpm/opentype.js@1.3.4/node_modules:/mnt/e/minna's kits/minnanaland/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../opentype.js/bin/ot" "$@"
else
  exec node  "$basedir/../opentype.js/bin/ot" "$@"
fi
