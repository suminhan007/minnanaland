#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/suminhan/Documents/project/minna's projects/minnanaland/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/suminhan/Documents/project/minna's projects/minnanaland/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vue-tsc@1.8.27_typescript@5.3.3/node_modules/vue-tsc/bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../../../../../vue-tsc@1.8.27_typescript@5.3.3/node_modules/vue-tsc/bin/vue-tsc.js" "$@"
fi
